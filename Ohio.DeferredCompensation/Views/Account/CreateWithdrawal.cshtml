@model WithdrawalRequestModel
@{
    ViewBag.Title = "Withdrawals Create Page";
    ViewBag.AtiveTab = NavigationTab.None;
    ViewBag.DarkOverlayClass = "account-manager-content";
    ViewBag.IsLoggedIn = true;
    ViewBag.UseSingleSocialImage = true;
}
@section scripts {
    @Scripts.Render("~/bundles/third-party-forms-js")
}
<div class="withdrawals-create all-page">
    @{ Html.RenderPartial("_TopIdentifyPanel", new TopIdentityPanelModel { ApplyXsXmClasses = true }); }
    @{ Html.RenderPartial("_SubNavigation", "Withdrawals"); }
    <div class="create-tab-one ">
        <div class="breadcrumb ver-y">
            <a href="@Url.Action("Withdrawals", "Account")">
                <span class="label titles">Create Withdrawal Request</span>
            </a>
        </div>
        <div class="step-by-step hidden-xs hidden-sm">
            <div class="nested-circle hol-x">
                <ul class="desk-step">
                    <li class="step01 current">
                        <div class="group-circle mix-xy clear-border">
                            <img class="first-sym" src="~/Content/i/icon-withdrawal-plan.png" alt="IMG">
                            <img class="second-sym" src="~/Content/i/icon-withdrawal-plan.png" alt="IMG">
                            <img class="first-cir" src="~/Content/i/progress25.png" alt="IMG">
                            <img class="second-cir" src="~/Content/i/progress100.png" alt="IMG">
                            <span class="icons icon-v"></span>
                        </div>
                        <div class="group-name">
                            <span class="name bold">Withdrawal Plan</span>
                        </div>
                    </li>
                    <li class="step02">
                        <div class="group-circle mix-xy">
                            <img class="first-sym" src="~/Content/i/icon-payment-options-black.png" alt="IMG">
                            <img class="second-sym" src="~/Content/i/icon-payment-options.png" alt="IMG">
                            <img class="first-cir" src="~/Content/i/progress25.png" alt="IMG">
                            <img class="second-cir" src="~/Content/i/progress100.png" alt="IMG">
                            <span class="icons icon-v"></span>
                        </div>
                        <div class="group-name">
                            <span class="name bold">Payment Options</span>
                        </div>
                    </li>
                    <li class="step03">
                        <div class="group-circle mix-xy">
                            <img class="first-sym" src="~/Content/i/seach-page01.png" alt="IMG">
                            <img class="second-sym" src="~/Content/i/seach-page.png" alt="IMG">
                            <img class="first-cir" src="~/Content/i/progress25.png" alt="IMG">
                            <img class="second-cir" src="~/Content/i/progress100.png" alt="IMG">
                            <span class="icons icon-v"></span>
                        </div>
                        <div class="group-name">
                            <span class="name bold clear-right">Review</span>
                        </div>
                    </li>
                    <li class="step04">
                        <div class="group-circle mix-xy">
                            <img class="first-sym" src="~/Content/i/indentifi01.png" alt="IMG">
                            <img class="second-sym" src="~/Content/i/indentifi.png" alt="IMG">
                            <img class="first-cir" src="~/Content/i/progress25.png" alt="IMG">
                            <img class="second-cir" src="~/Content/i/progress100.png" alt="IMG">
                            <span class="icons icon-v"></span>
                        </div>
                        <div class="group-name">
                            <span class="name bold">Identification Code</span>
                        </div>
                    </li>
                    <li class="step05">
                        <div class="group-circle mix-xy">
                            <img class="first-sym" src="~/Content/i/enter-code01.png" alt="IMG">
                            <img class="second-sym" src="~/Content/i/enter-code.png" alt="IMG">
                            <img class="first-cir" src="~/Content/i/progress25.png" alt="IMG">
                            <img class="second-cir" src="~/Content/i/progress100.png" alt="IMG">
                            <span class="icons icon-v"></span>
                        </div>
                        <div class="group-name">
                            <span class="name bold">Enter Code</span>
                        </div>
                    </li>
                </ul>
                <a href="javascript:;" class="click-intructions">
                    <img src="~/Content/i/mobi/icons-intructions.png" alt="IMG">
                </a>
            </div>
            <!-- end /nested-circle -->
            <div class="step-more-details">
                @using (Html.BeginForm("CreateWithdrawal", "Account", FormMethod.Post))
                {
                    <input type="hidden" name="AccessCode" value="code" />
                    <div class="step-content step-one-content ">
                        <div class="top-title">
                            <div class="title">Step 1 : Select Withdrawal Plan</div>
                        </div>
                        <!-- end /top-title -->
                        <div class="middle-form">
                            <div class="form-order01">
                                <div class="box-radio">
                                    <input type="radio" id="boxx01" name="@nameof(WithdrawalRequestModel.Plan)" value="OngoingOrRecurrentPayment" checked="checked">
                                    <label for="boxx01" class="big-radio">
                                        <span>Ongoing/Recurring Payment</span>
                                    </label>
                                    <input type="radio" id="boxx02" name="@nameof(WithdrawalRequestModel.Plan)" value="OneTimePayment">
                                    <label for="boxx02" class="big-radio">
                                        <span>One-Time Payment</span>
                                    </label>
                                </div>
                                <!-- box radio -->
                            </div>
                            <!-- end /form-order01 -->
                            @{ Html.RenderPartial("_BoxInstructions", new InstructionsBoxModel(ViewBag.DesktopStep1Instructions, cssClass: "hidden-xs hidden-sm")); }
                            <div class="clear"></div>
                        </div>
                        <!-- end /middle-form -->
                        <div class="bottom-button">
                            <a href="javascript:;" class="btn-cancel mix-xy">Cancel</a>
                            <a href="javascript:;" class="btn-next mix-xy btn-next-step">Next</a>
                            <div class="clear"></div>
                        </div>
                        <!-- end /bottom-button -->
                    </div>
                    <!-- end /step-one-content -->
                                <div class="step-content step-one-content payment-step hide">
                                    <div class="top-title">
                                        <div class="title">Step 2 : Payment Option</div>
                                    </div>
                                    <!-- end /top-title -->
                                    <div class="middle-form">
                                        <div class="form-order01">
                                            <ul>
                                                <li class="border-bottom height-55">
                                                    <span class="name ver-y bold">Withdrawal Date</span>
                                                    <div class="typing-here start-withdrawal-date">
                                                        @Html.CustomTextBoxFor(x => x.StartDate, "dd/mm/yy", @class: "iinput datepicker-birth start-withdrawal-date", id: "start-withdrawal-date01")
                                                        <button class="icons select-calculator"></button>
                                                    </div>
                                                    <label for="start-withdrawal-date01" class="hide">Withdrawal Date</label>
                                                </li>
                                                <li class="border-bottom height-55 payment-frequency-hide">
                                                    <span class="name ver-y bold">Desired Payment Frequency</span>
                                                    <div class="typing-here desired-payment-frequency">
                                                        <div class="box-radio">
                                                            <input type="radio" id="boxx03" name="@nameof(WithdrawalRequestModel.PaymentFrequency)" value="Monthly">
                                                            <label for="boxx03" class="big-radio">
                                                                <span>Monthly</span>
                                                            </label>
                                                            <input type="radio" id="boxx04" name="@nameof(WithdrawalRequestModel.PaymentFrequency)" value="Quarterly">
                                                            <label for="boxx04" class="big-radio">
                                                                <span>Quarterly</span>
                                                            </label>
                                                            <input type="radio" id="boxx05" name="@nameof(WithdrawalRequestModel.PaymentFrequency)" value="SemiAnnually">
                                                            <label for="boxx05" class="big-radio">
                                                                <span>Semi-Annually</span>
                                                            </label>
                                                            <input type="radio" id="boxx06" name="@nameof(WithdrawalRequestModel.PaymentFrequency)" value="Annually" checked="checked">
                                                            <label for="boxx06" class="big-radio">
                                                                <span>Annually</span>
                                                            </label>
                                                        </div>
                                                    </div>
                                                </li>
                                                <li class="border-bottom height-150">
                                                    <span class="name bold">Enter Payment Option</span>
                                                    <div class="typing-here enter-payment-option">
                                                        <div class="box-radio">
                                                            <input type="radio" id="boxxpaymentoption01" name="@nameof(WithdrawalRequestModel.PaymentOption)" value="DollarAmount" checked="checked">
                                                            <label for="boxxpaymentoption01" class="big-radio">
                                                                <span>Dollar Ammount</span>
                                                            </label>
                                                            <input type="radio" id="boxxpaymentoption02" name="@nameof(WithdrawalRequestModel.PaymentOption)" value="PercentToWithdraw">
                                                            <label for="boxxpaymentoption02" class="big-radio">
                                                                <span>% of account value  to be withdrawn</span>
                                                            </label>
                                                            <input type="radio" id="boxxpaymentoption03" name="@nameof(WithdrawalRequestModel.PaymentOption)" value="TimePeriod">
                                                            <label for="boxxpaymentoption03" class="big-radio">
                                                                <span>Time Period</span>
                                                            </label>
                                                        </div>
                                                        <div class="input-container">
                                                            <div>
                                                                $@Html.CustomTextBoxFor(x => x.PaymentOptionDollarAmount, id: "payment01")
                                                                <label for="payment01" class="hide">Label</label>
                                                            </div>
                                                            <div>
                                                                @Html.CustomTextBoxFor(x => x.PaymentOptionPercentToWithdraw, @class: "width64 percentage-type disabled", id: "percent01")%
                                                                <label for="percent01" class="hide">Label</label>
                                                            </div>
                                                            <div>
                                                                @Html.CustomTextBoxFor(x => x.PaymentOptionTimePeriod, @class: "width64 disabled", id: "years01")years
                                                                <label for="years01" class="hide">Label</label>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </li>
                                                <li class="border-bottom  height-100">
                                                    <span class="name bold">Fund Selection</span>
                                                    <div class="typing-here fund-selection">
                                                        <div class="box-radio clearfix">
                                                            <input type="radio" id="boxxfundselection04" name="@nameof(WithdrawalRequestModel.FundSelectionOption)" value="AllFunds" checked="checked">
                                                            <label for="boxxfundselection04" class="big-radio">
                                                                <span>All  Funds (Pro-Rata)</span>
                                                            </label>
                                                            <input type="radio" id="boxxfundselection05" name="@nameof(WithdrawalRequestModel.FundSelectionOption)" value="ChooseFund">
                                                            <label for="boxxfundselection05" class="big-radio">
                                                                <span>Choose Fund</span>
                                                            </label>
                                                        </div>
                                                        <div class="input-container">
                                                            <div>
                                                            </div>
                                                            <div>
                                                                <div class="dropdown-here ddown-here disabled">
                                                                    @Html.CustomSelectFor(x => x.FundSelectionSelectedFund, (IList<string>)ViewBag.FundNames, "Select Fund", nullValue: "SELECT", @class: "ddown-select big-text hide", id: "ui-id-1")
                                                                    <span class="icons-ddown"></span>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </li>
                                                <li class="border-bottom height-306">
                                                    <span class="name ver-y bold auto-height">Bank Deposit Information</span>
                                                    <button class="modify-btn" type="button">MODIFY</button>
                                                    <div class="typing-here bank-deposit-information">
                                                        <ul>
                                                            <li>
                                                                <span class="name ver-y">Direct Deposit or Check</span>
                                                                <div class="typing-here desired-payment-frequency">
                                                                    <div class="box-radio">
                                                                        <input type="radio" id="directdepositorcheck01" name="@nameof(WithdrawalRequestModel.DepositOption)" value="DirectDeposit" checked="checked">
                                                                        <label for="directdepositorcheck01" class="big-radio">
                                                                            <span>Direct Deposit</span>
                                                                        </label>
                                                                        <input type="radio" id="directdepositorchec02" name="@nameof(WithdrawalRequestModel.DepositOption)" value="Check">
                                                                        <label for="directdepositorchec02" class="big-radio">
                                                                            <span>Check</span>
                                                                        </label>
                                                                    </div>
                                                                </div>
                                                            </li>
                                                            <li>
                                                                <span class="name ver-y">Routing #</span>
                                                                <div class="typing-here routing">
                                                                    @Html.CustomTextBoxFor(x => x.NewDepositRoutingNumber, "Enter Routing #", @class: "iinput hide", id: "routing-id1")
                                                                    <span>@Model.DepositRoutingNumber</span>
                                                                </div>
                                                                <label for="routing-id1" class="hide">Routing #</label>
                                                            </li>
                                                            <li>
                                                                <span class="name ver-y">Bank Account #</span>
                                                                <div class="typing-here backaccount">
                                                                    @Html.CustomTextBoxFor(x => x.NewDepositBankAccount, "Enter Bank Account #", @class: "iinput hide", id: "bank-account-id1")
                                                                    <span>@Model.DepositBankAccount</span>
                                                                </div>
                                                                <label for="bank-account-id1" class="hide">Bank Account #</label>
                                                            </li>
                                                            <li>
                                                                <span class="name ver-y">Bank Name</span>
                                                                <div class="typing-here bankname">
                                                                    <span>@Model.DepositBankName</span>
                                                                </div>
                                                            </li>
                                                            <li>
                                                                <span class="name ver-y">Type or Account</span>
                                                                <div class="typing-here desired-payment-frequency">
                                                                    <div class="box-radio">
                                                                        <input type="radio" id="typeoraccount01" name="@nameof(WithdrawalRequestModel.DepositTypeOrAccount)" value="Checking" checked="checked">
                                                                        <label for="typeoraccount01" class="big-radio">
                                                                            <span>Checking</span>
                                                                        </label>
                                                                        <input type="radio" id="typeoraccount02" name="@nameof(WithdrawalRequestModel.DepositTypeOrAccount)" value="Savings">
                                                                        <label for="typeoraccount02" class="big-radio">
                                                                            <span>Saveings</span>
                                                                        </label>
                                                                    </div>
                                                                </div>
                                                            </li>
                                                        </ul>
                                                        <div class="tip-info hide">
                                                            <span class="icons tip"></span> Lumpsum or Annual Payment only for the check payment method
                                                        </div>
                                                    </div>
                                                </li>
                                                <li class="border-bottom height-255">
                                                    <span class="name ver-y bold width-per-100 auto-height">Federal Tax Withholding Information</span>
                                                    <div class="typing-here bank-deposit-information">
                                                        <ul>
                                                            <li>
                                                                <span class="name ver-y">Federal Tax Exempt</span>
                                                                <div class="typing-here desired-payment-frequency">
                                                                    <div class="box-radio">
                                                                        <input type="radio" id="federaltaxexempt01" name="@nameof(WithdrawalRequestModel.FederalTaxExempt)" value="true" checked="checked">
                                                                        <label for="federaltaxexempt01" class="big-radio">
                                                                            <span>Yes</span>
                                                                        </label>
                                                                        <input type="radio" id="federaltaxexempt02" name="@nameof(WithdrawalRequestModel.FederalTaxExempt)" value="false">
                                                                        <label for="federaltaxexempt02" class="big-radio">
                                                                            <span>No</span>
                                                                        </label>
                                                                    </div>
                                                                </div>
                                                            </li>
                                                            <li class="height-200">
                                                                <label for="withholding-percentage01"
                                                                       class="name ver-y name-withholding-percentage">Withholding  Percentage</label>
                                                                <div class="typing-here withholdingpercentage clearfix">
                                                                    <div class="input-container-left">
                                                                        @Html.CustomTextBoxFor(x => x.FederalTaxWithholdingPercentage, @class: "iinput percentage-type", id: "withholding-percentage01")%
                                                                    </div>
                                                                    <div class="divider">
                                                                        <span class="or-txt">(Or)</span>
                                                                    </div>
                                                                    <div class="input-container-right">
                                                                        <ul>
                                                                            <li>
                                                                                <span class="name ver-y">Marital Status</span>
                                                                                <div class="typing-here desired-payment-frequency">
                                                                                    <div class="box-radio">
                                                                                        <input type="radio" id="maritalstatus01" name="@nameof(WithdrawalRequestModel.MaritalStatus)" value="true" checked="checked">
                                                                                        <label for="maritalstatus01" class="big-radio">
                                                                                            <span>Yes</span>
                                                                                        </label>
                                                                                        <input type="radio" id="maritalstatus02" name="@nameof(WithdrawalRequestModel.MaritalStatus)" value="false">
                                                                                        <label for="maritalstatus02" class="big-radio">
                                                                                            <span>No</span>
                                                                                        </label>
                                                                                    </div>
                                                                                </div>
                                                                            </li>
                                                                            <li>
                                                                                <label for="number-of-allowances" class="name ver-y">Number  Of Allowances</label>
                                                                                <div class="typing-here">
                                                                                    @Html.CustomTextBoxFor(x => x.FederalTaxNumberOfAllowances, @class: "iinput width64", id: "number-of-allowances")
                                                                                </div>
                                                                            </li>
                                                                            <li>
                                                                                <label for="additional-withholding" class="name ver-y">Additional  Withholding</label>
                                                                                <div class="typing-here">
                                                                                    $@Html.CustomTextBoxFor(x => x.FederalTaxAdditionalWithholding, @class: "iinput width124", id: "additional-withholding")
                                                                                </div>
                                                                            </li>
                                                                        </ul>
                                                                    </div>
                                                                </div>
                                                            </li>
                                                        </ul>
                                                    </div>
                                                </li>
                                                <li class="border-bottom height-225">
                                                    <span class="name ver-y bold auto-height width-per-100">State of Ohio Tax Withholding Information</span>
                                                    <div class="typing-here bank-deposit-information">
                                                        <ul>
                                                            <li>
                                                                <span class="name ver-y">State Tax Exempt</span>
                                                                <div class="typing-here desired-payment-frequency">
                                                                    <div class="box-radio">
                                                                        <input type="radio" id="statetaxexempt01" name="@nameof(WithdrawalRequestModel.StateTaxExempt)" value="true" checked="checked">
                                                                        <label for="statetaxexempt01" class="big-radio">
                                                                            <span>Yes</span>
                                                                        </label>
                                                                        <input type="radio" id="statetaxexempt02" name="@nameof(WithdrawalRequestModel.StateTaxExempt)" value="false">
                                                                        <label for="statetaxexempt02" class="big-radio">
                                                                            <span>No</span>
                                                                        </label>
                                                                    </div>
                                                                </div>
                                                            </li>
                                                            <li>
                                                                <div class="typing-here enter-payment-option">
                                                                    <div class="box-radio">
                                                                        <input type="radio" id="statetaxwithholding01" name="@nameof(WithdrawalRequestModel.StateTaxOption)" value="NumberOfAllowances" checked="checked">
                                                                        <label for="statetaxwithholding01" class="big-radio">
                                                                            <span>Number Of Allowances</span>
                                                                        </label>
                                                                        <input type="radio" id="statetaxwithholding02" name="@nameof(WithdrawalRequestModel.StateTaxOption)" value="WithholdingAmount">
                                                                        <label for="statetaxwithholding02" class="big-radio">
                                                                            <span>Withholding Dollar Amount</span>
                                                                        </label>
                                                                        <input type="radio" id="statetaxwithholding03" name="@nameof(WithdrawalRequestModel.StateTaxOption)" value="WithholdingPercentage">
                                                                        <label for="statetaxwithholding03" class="big-radio">
                                                                            <span>Witholding Percentage</span>
                                                                        </label>
                                                                    </div>
                                                                    <div class="input-container">
                                                                        <div>
                                                                            @Html.CustomTextBoxFor(x => x.StateTaxNumberOfAllowances, @class: "width64", id: "payment02")
                                                                            <label for="payment02" class="hide">Label</label>
                                                                        </div>
                                                                        <div>
                                                                            $@Html.CustomTextBoxFor(x => x.StateTaxWithholdingAmount, @class: "disabled", id: "percent02")
                                                                            <label for="percent02" class="hide">Label</label>
                                                                        </div>
                                                                        <div>
                                                                            @Html.CustomTextBoxFor(x => x.StateTaxWithholdingPercentage, @class: "width64 percentage-type disabled", id: "years02")%
                                                                            <label for="years02" class="hide">Label</label>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </li>
                                                        </ul>
                                                    </div>
                                                </li>
                                            </ul>
                                        </div>
                                        <!-- end /form-order01 -->
                                        @{ Html.RenderPartial("_BoxInstructions", new InstructionsBoxModel(ViewBag.DesktopStep2Instructions, cssClass: "hidden-xs hidden-sm height1000")); }
                                        <div class="clear"></div>
                                    </div>
                                    <!-- end /middle-form -->
                                    <div class="bottom-button">
                                        <a href="javascript:;" class="btn-cancel mix-xy">PREVIOUS</a>
                                        <a href="javascript:;" class="btn-next mix-xy validate-all-positive-numeric validate-model-form">NEXT</a>
                                        <div class="clear"></div>
                                    </div>
                                    <!-- end /bottom-button -->
                                </div>
                    <!-- end /step-two-content -->
                                            <div class="step-content step-three-content hide">
                                                <div class="top-title">
                                                    <div class="title">Step 3 : Review</div>
                                                </div>
                                                <!-- end /top-title -->
                                                <div class="middle-form">
                                                    <div class="form-order01">
                                                        <div class="row">
                                                            <ul>
                                                                <li>
                                                                    <span class="title">Withdrawal Date</span>
                                                                    <span class="descr">10/01/2017</span>
                                                                </li>
                                                                <li class="payment-frequency-hide">
                                                                    <span class="title">Desired Payment Frequency</span>
                                                                    <span class="descr">Monthly</span>
                                                                </li>
                                                                <li>
                                                                    <span class="title">Enter Payment Option</span>
                                                                    <span class="descr">Dollar Acount<em>$ 1,000.00</em></span>
                                                                </li>
                                                                <li>
                                                                    <span class="title">Gross Amount</span>
                                                                    <span class="descr">$ 1,000.00</span>
                                                                </li>
                                                                <li>
                                                                    <span class="title">Net Amount</span>
                                                                    <span class="descr">$ 970.40</span>
                                                                </li>
                                                                <li>
                                                                    <span class="title">Fund Selection</span>
                                                                    <span class="descr">All Funds(Pro-Rata)</span>
                                                                </li>
                                                            </ul>
                                                        </div>
                                                        <div class="row">
                                                            <h5>
                                                                Bank Deposit Information
                                                            </h5>
                                                            <ul class="padding20">
                                                                <li>
                                                                    <span class="title">Direct Deposit or Check</span>
                                                                    <span class="descr">Direct Deposit</span>
                                                                </li>
                                                                <li>
                                                                    <span class="title">Routing #</span>
                                                                    <span class="descr">****6789</span>
                                                                </li>
                                                                <li>
                                                                    <span class="title">Bank Account #</span>
