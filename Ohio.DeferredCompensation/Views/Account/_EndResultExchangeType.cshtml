                                            @Html.CustomTextBoxFor(x => x.NewOhioDCSmallCapGrowth, "Enter Value", @class: "number-only")
                                            <span class="text-percent">%</span>
                                        </div>
                                    </div>
                                    <span class="topic-title">
                                        Mid Cap Stocks
                                        <span class="icons arrow-right"></span>
                                    </span>
                                    <div class="box-content">
                                        <span class="nameone">Vanguard Capital Opportunity</span>
                                        <span class="text-investment right">@Model.VanguardCapitalOpportunity%</span>
                                        <div class="input-here">
                                            @Html.CustomTextBoxFor(x => x.NewVanguardCapitalOpportunity, "Enter Value", @class: "number-only")
                                            <span class="text-percent">%</span>
                                        </div>
                                    </div>
                                    <span class="topic-title">
                                        Large Cap Stocks
                                        <span class="icons arrow-right"></span>
                                    </span>
                                    <div class="box-content">
                                        <span class="nameone">Dodge & Cox Stocks</span>
                                        <span class="text-investment right">@Model.DodgeAndCoxStocks%</span>
                                        <div class="input-here">
                                            @Html.CustomTextBoxFor(x => x.NewDodgeAndCoxStocks, "Enter Value", @class: "number-only")
                                            <span class="text-percent">%</span>
                                        </div>
                                    </div>
                                    <div class="box-content two">
                                        <span class="nameone">Fidelity Contrafund</span>
                                        <span class="text-investment right">@Model.FidelityContrafund%</span>
                                        <div class="input-here">
                                            @Html.CustomTextBoxFor(x => x.NewFidelityContrafund, "Enter Value", @class: "number-only")
                                            <span class="text-percent">%</span>
                                        </div>
                                    </div>
                                    <div class="box-content">
                                        <span class="nameone">Fidelity Growth Company</span>
                                        <span class="text-investment right">@Model.FidelityGrowthCompany%</span>
                                        <div class="input-here">
                                            @Html.CustomTextBoxFor(x => x.NewFidelityGrowthCompany, "Enter Value", @class: "number-only")
                                            <span class="text-percent">%</span>
                                        </div>
                                    </div>
                                    <div class="box-content two">
                                        <span class="nameone">Ohio DC Large Cap Growth</span>
                                        <span class="text-investment right">@Model.OhioDCLargeCapGrowth%</span>
                                        <div class="input-here">
                                            @Html.CustomTextBoxFor(x => x.NewOhioDCLargeCapGrowth, "Enter Value", @class: "number-only")
                                            <span class="text-percent">%</span>
                                        </div>
                                    </div>
                                    <span class="topic-title">
                                        Asset allocation funds
                                        <span class="icons arrow-right"></span>
                                    </span>
                                    <div class="box-content">
                                        <span class="nameone">Lifepath 2020</span>
                                        <span class="text-investment right">@Model.Lifepath2020%</span>
                                        <div class="input-here">
                                            @Html.CustomTextBoxFor(x => x.NewLifepath2020, "Enter Value", @class: "number-only")
                                            <span class="text-percent">%</span>
                                        </div>
                                    </div>
                                    <span class="topic-title">
                                        Bonds
                                        <span class="icons arrow-right"></span>
                                    </span>
                                    <div class="box-content">
                                        <span class="nameone">Ohio DC Intermediate Bond</span>
                                        <span class="text-investment right">@Model.OhioDCIntermediateBond%</span>
                                        <div class="input-here">
                                            @Html.CustomTextBoxFor(x => x.NewOhioDCIntermediateBond, "Enter Value", @class: "number-only")
                                            <span class="text-percent">%</span>
                                        </div>
                                    </div>
                                    <div class="box-content two">
                                        <span class="nameone">Vanguard Total Bond Market Index</span>
                                        <span class="text-investment right">@Model.VanguardTotalBondMarketIndex%</span>
                                        <div class="input-here">
                                            @Html.CustomTextBoxFor(x => x.NewVanguardTotalBondMarketIndex, "Enter Value", @class: "number-only")
                                            <span class="text-percent">%</span>
                                        </div>
                                    </div>

