@model EndResultExchangeModel
@{
    var jss = new System.Web.Script.Serialization.JavaScriptSerializer();
    var modelJson = jss.Serialize(Model);
}
<input type="hidden" id="end-result-exchange-model" value="@modelJson" />
<div class="step-by-step request-fund hidden-xs hidden-sm hide">
