                                            <span class="nametwo">Vanguard Capital Opportunity</span><span class="right">@(Model.VanguardCapitalOpportunity?.ToString("N2"))%</span>
                                        </div>
                                    </div>
                                    <!-- content fund -->
                                </div>
                                <div class="right">
                                    <div class="report width140">
                                        <div class="pie-chart"></div> New Investment
                                    </div>
                                    <div class="box-auto ver-y">
                                        <span class="title">NEW INVESTMENT</span>
                                    </div>
                                    <!-- box auto -->
                                    <div class="content-fund reviewfund">
                                        <div class="top-c-fund">
                                            <span class="percent reviewper">
                                                <span class="title">Fund Name</span><span class="next">Percentage</span>
                                            </span>
                                        </div>
                                        <!-- top c fund -->
                                        <span class="topic-title">International Stocks</span>
                                        <div class="box-content">
                                            <span class="nametwo">Templeton Foreign</span><span class="right">30.00%</span>
                                        </div>
                                        <span class="topic-title">Small Cap Stocks</span>
                                        <div class="box-content">
                                            <span class="nametwo">Ohio DC Large Cap Growth</span> <span class="right">40.00%</span>
                                        </div>
                                        <span class="topic-title">Mid Cap Stocks</span>
                                        <div class="box-content">
                                            <span class="nametwo">Vanguard Capital Opportunity</span> <span class="right">30.00%</span>
                                        </div>
                                    </div>
                                    <!-- content fund -->
                                </div>
                            </div>
                            @{ Html.RenderPartial("_BoxInstructions", new InstructionsBoxModel(ViewBag.DesktopEndResultStep3Instructions, cssClass: "hidden-xs hidden-sm")); }
                            <div class="clear"></div>
                        </div>
                        <!-- second content -->
                    </div>
                    <!-- top content -->
                </div>
                <!-- cover content -->
                <div class="bottom-button">
                    <a href="javascript:;" class="btn-cancel mix-xy btn-previous-step">Previous</a>
                    <div class="right-btn">
                        <a href="javascript:;" class="btn-next mix-xy btn-next-step btn-submit-form-ajax">FINISH AND CONFIRM</a>
                    </div>
                    <div class="clear"></div>
                </div>
                <!-- end /bottom-button -->
            </div>
            <!-- end /step-one-content -->
            <div class="step-content step-completed-content hide">
                <div class="top-title">
                    <div class="title">Completed</div>
                </div>
                <!-- end /top-title -->
                <div class="middle-form">
                    <div class="form-order02">
                        <div class="content-left">
                            <span class="big-text">Successful!</span>
                            <div class="group-notice">
                                <img src="~/Content/i/select-box.png" alt="IMG">
                                <p>
                                    Your Fund Investments Reallocation has been
                                    successfully created.
                                </p>
                            </div>
                            <!-- end /group-notice -->
                            <a href="@Url.Action("FundExchange", "Account")" class="btn-view mix-xy">VIEW FUND EXCHANGES</a>
                        </div>
                    </div>
                    <!-- end /form-order01 -->
                    @{ Html.RenderPartial("_BoxInstructions", new InstructionsBoxModel(ViewBag.DesktopEndResultStepCompletedInstructions, cssClass: "hidden-xs hidden-sm")); }
                    <div class="clear"></div>
                </div>
                <!-- end /middle-form -->
            </div>
            <!-- end /step two content -->
        </div>
        <!-- end /step-more-details -->
    }
</div>
<!-- end /step-by-step -->

