                <div class="middle-form">
                    <div class="form-order01">
                        <div class="box-radio">
                            <input type="radio" id="boxx06" name="ivest02">
                            <label for="boxx06" class="big-radio">
                                <span>Fund Exchange</span>
                            </label>
                            <input type="radio" id="boxx07" name="ivest02">
                            <label for="boxx07" class="big-radio">
                                <span>End Result Exchange</span>
                            </label>
                        </div>
                        <!-- box radio -->
                    </div>
                    <!-- end /form-order01 -->
                    @{ Html.RenderPartial("_BoxInstructions", new InstructionsBoxModel(ViewBag.DesktopEndResultStep1Instructions, cssClass: "hidden-xs hidden-sm")); }
                    <div class="clear"></div>
                </div>
                <!-- end /middle-form -->
                <div class="bottom-button">
                    <a href="javascript:;" class="btn-cancel mix-xy">Cancel</a>
                    <a href="javascript:;" class="btn-next mix-xy btn-next-step">Next</a>
                    <div class="clear"></div>
                </div>
                <!-- end /bottom-button -->
            </div>
            <!-- end /step-one-content -->
            <div class="step-content step-two-content hide">
                <div class="top-title">
                    <div class="title">Step 2 : Choose New Investment</div>
                </div>
